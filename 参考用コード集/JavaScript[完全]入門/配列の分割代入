
// 配列を作成
const arr = [ 'コーヒー' , '紅茶' , ' ジュース' , 'ほうじ茶' , 'そば茶' ];

// 要素を飛ばして分割代入
const [a,, b, c] = arr;
// 残った値をすべて分割代入
const [d, e, ...f] = arr;
// 規定値を設定して分割代入
const [,,,,,g = 'ルイボス茶'] = arr;

// コンソールに出力
console.log(a,b,c);
console.log(d,e,f);
console.log(g);



// 結果
コーヒー  ジュース ほうじ茶
コーヒー 紅茶 (3) [' ジュース', 'ほうじ茶', 'そば茶']
ルイボス茶

