

// オブジェクトを作成
const obj = { menuA : 'コーヒー' ,  menuB : '紅茶' ,  menuC : 'ジュース' } ;

// 分割代入
const {menuA , menuB, menuC} = obj;
// 変数名を変えて、分割代入
const {menuA:a , menuB:b, menuC:c} = obj;
// 規定値を設定して、分割代入
const {menuD = 'ほうじ茶', menuE = 'そば茶'} = obj

console.log(menuA, menuB, menuC);
console.log(a,b,c);
console.log(menuD , menuE);



// 結果
コーヒー 紅茶 ジュース
コーヒー 紅茶 ジュース
ほうじ茶 そば茶


